pipeline {
    agent any
            environment {
AWS _CREDENTIALS = credentials('30586e3b-bcc6-40fc-aa73-e7388a2c193e')
            }
    stages {
        stage('Checkout') {
            steps {
               checkout scm
            }
        }
        stage('Terraform init') {
            steps {
                sh 'terraform init'
            }
        }
        stage('Terraform apply') {
            steps {
                sh 'terraform apply --auto-approve'
            }
        }
        
    }
}
